<ResourceDictionary xmlns="https://github.com/avaloniaui"
                    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
					xmlns:cc="clr-namespace:NavigationTabShowCase.Controls;assembly=NavigationTabShowCase">
	<Design.PreviewWith>
		<cc:NavigationActionStripItem Title="XYETA">
		</cc:NavigationActionStripItem>
	</Design.PreviewWith>
	<ControlTheme x:Key="{x:Type cc:NavigationActionStripItem}" TargetType="cc:NavigationActionStripItem">
		<Setter Property="Background" Value="Yellow" />
		<Setter Property="BorderBrush" Value="Blue" />
		<Setter Property="BorderThickness" Value="10" />
		<Setter Property="Template">
			<ControlTemplate>
				<Border 
					BorderBrush="{TemplateBinding BorderBrush}"
					BorderThickness="{TemplateBinding BorderThickness}"
					CornerRadius="{TemplateBinding CornerRadius}">
					<TextBlock
						Name="PART_TextBlock"
						Text="{TemplateBinding Title}">
					</TextBlock>
				</Border>
			</ControlTemplate>
		</Setter>
	</ControlTheme>
</ResourceDictionary>
